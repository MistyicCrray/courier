<view><cmd-nav-bar vue-id="1" back title="信息设置" bind:__l="__l"></cmd-nav-bar><cmd-page-body vue-id="2" type="top" bind:__l="__l" vue-slots="{{['default']}}"><cmd-transition vue-id="{{('3')+','+('2')}}" name="fade-up" bind:__l="__l" vue-slots="{{['default']}}"><view><cmd-cel-item vue-id="{{('4')+','+('3')}}" title="头像" slot-right arrow data-event-opts="{{[['^click',[['chooseImage']]]]}}" bind:click="__e" bind:__l="__l" vue-slots="{{['default']}}"><cmd-avatar vue-id="{{('5')+','+('4')}}" src="{{userInfo.avatar==''?'https://avatar.bbs.miui.com/images/noavatar_small.gif':userInfo.avatar}}" bind:__l="__l"></cmd-avatar></cmd-cel-item><cmd-cel-item vue-id="{{('6')+','+('3')}}" title="昵称" addon="{{userInfo.userName}}" arrow bind:__l="__l"></cmd-cel-item><cmd-cel-item vue-id="{{('7')+','+('3')}}" title="邮箱" addon="{{userInfo.email}}" arrow bind:__l="__l"></cmd-cel-item><cmd-cel-item vue-id="{{('8')+','+('3')}}" title="修改个人信息" arrow data-event-opts="{{[['^click',[['updateInfo']]]]}}" bind:click="__e" bind:__l="__l"></cmd-cel-item><cmd-cel-item vue-id="{{('9')+','+('3')}}" title="修改密码" arrow data-event-opts="{{[['^click',[['fnClick',['']]]]]}}" bind:click="__e" bind:__l="__l"></cmd-cel-item><button data-event-opts="{{[['tap',[['logout']]]]}}" class="btn-logout" bindtap="__e">退出登录</button></view></cmd-transition></cmd-page-body></view>