<view><cmd-nav-bar vue-id="1" back title="密码重置" bind:__l="__l"></cmd-nav-bar><cmd-page-body vue-id="2" type="top" bind:__l="__l" vue-slots="{{['default']}}"><cmd-transition vue-id="{{('3')+','+('2')}}" name="fade-up" bind:__l="__l" vue-slots="{{['default']}}"><view class="forgot"><view class="forgot-phone"><cmd-input bind:input="__e" vue-id="{{('4')+','+('3')}}" type="text" placeholder="请输入邮箱" value="{{user.email}}" data-event-opts="{{[['^input',[['__set_model',['$0','email','$event',[]],['user']]]]]}}" bind:__l="__l"></cmd-input><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" class="forgot-phone-getcode" bindtap="__e">{{!safety.state&&'获取验证码'||safety.time+' s'}}</view></view><view class="forgot-code"><cmd-input bind:input="__e" vue-id="{{('5')+','+('3')}}" type="number" maxlength="6" placeholder="请输入验证码" value="{{user.code}}" data-event-opts="{{[['^input',[['__set_model',['$0','code','$event',[]],['user']]]]]}}" bind:__l="__l"></cmd-input></view><view class="forgot-password"><cmd-input bind:input="__e" vue-id="{{('6')+','+('3')}}" type="password" displayable maxlength="26" placeholder="请输入新密码" value="{{user.passwordOne}}" data-event-opts="{{[['^input',[['__set_model',['$0','passwordOne','$event',[]],['user']]]]]}}" bind:__l="__l"></cmd-input></view><view class="forgot-password"><cmd-input bind:input="__e" vue-id="{{('7')+','+('3')}}" type="password" maxlength="26" placeholder="请再次确认新密码" value="{{user.passwordTwo}}" data-event-opts="{{[['^input',[['__set_model',['$0','passwordTwo','$event',[]],['user']]]]]}}" bind:__l="__l"></cmd-input></view><button class="btn-forgot" hover-class="btn-forgot-hover" data-event-opts="{{[['tap',[['fnForgot',['$event']]]]]}}" bindtap="__e">提交</button></view></cmd-transition></cmd-page-body></view>