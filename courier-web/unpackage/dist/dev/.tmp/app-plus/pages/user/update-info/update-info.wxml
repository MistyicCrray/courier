<view><cmd-nav-bar vue-id="1" back title="修改个人信息" bind:__l="__l"></cmd-nav-bar><cmd-page-body vue-id="2" type="top" bind:__l="__l" vue-slots="{{['default']}}"><view class="cu-form-group margin-top"><view class="title">昵称</view><input placeholder="昵称" name="input" data-event-opts="{{[['input',[['__set_model',['$0','userName','$event',[]],['user']]]]]}}" value="{{user.userName}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">专业</view><input placeholder="专业" name="input" data-event-opts="{{[['input',[['__set_model',['$0','specialty','$event',[]],['user']]]]]}}" value="{{user.specialty}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">班级</view><input placeholder="班级" name="input" data-event-opts="{{[['input',[['__set_model',['$0','className','$event',[]],['user']]]]]}}" value="{{user.className}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">手机号</view><input placeholder="手机号" name="input" data-event-opts="{{[['input',[['__set_model',['$0','phone','$event',[]],['user']]]]]}}" value="{{user.phone}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">电子邮件</view><input placeholder="电子邮件" name="input" data-event-opts="{{[['input',[['__set_model',['$0','email','$event',[]],['user']]]]]}}" value="{{user.email}}" bindinput="__e"/></view><view class="cu-bar bg-white margin-top"><view class="action">学生证</view></view><view class="cu-form-group"><view class="grid col-4 grid-square flex-sub"><block wx:for="{{imgList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="bg-img" data-url="{{imgList[index]}}" data-event-opts="{{[['tap',[['ViewImage',['$event']]]]]}}" bindtap="__e"><image src="{{imgList[index]}}" mode="aspectFill"></image><view class="cu-tag bg-red" data-index="{{index}}" data-event-opts="{{[['tap',[['DelImg',['$event']]]]]}}" catchtap="__e"><text class="cuIcon-close"></text></view></view></block><block wx:if="{{imgList.length<4}}"><view data-event-opts="{{[['tap',[['ChooseImage',['$event']]]]]}}" class="solids" bindtap="__e"><text class="cuIcon-cameraadd"></text></view></block></view></view><button data-event-opts="{{[['tap',[['submit']]]]}}" class="btn-register" bindtap="__e">提交</button></cmd-page-body></view>