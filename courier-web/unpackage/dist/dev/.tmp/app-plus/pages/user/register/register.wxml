<view><cmd-nav-bar vue-id="1" back title="用户注册" bind:__l="__l"></cmd-nav-bar><cmd-page-body vue-id="2" type="top" bind:__l="__l" vue-slots="{{['default']}}"><view class="register"><view class="register-title">{{status?'手机快捷注册':'账号密码注册'}}</view><view class="register-explain">{{status?'用户可通过手机验证码直接注册':'使用账号密码注册后请绑定手机号'}}</view><cmd-transition vue-id="{{('3')+','+('2')}}" name="fade-up" bind:__l="__l" vue-slots="{{['default']}}"><block wx:if="{{!status}}"><view><view class="register-username"><cmd-input bind:input="__e" vue-id="{{('4')+','+('3')}}" type="text" focus maxlength="26" placeholder="请输入账号" value="{{account.loginName}}" data-event-opts="{{[['^input',[['__set_model',['$0','loginName','$event',[]],['account']]]]]}}" bind:__l="__l"></cmd-input></view><view class="register-password"><cmd-input bind:input="__e" vue-id="{{('5')+','+('3')}}" type="password" displayable maxlength="26" placeholder="请输入密码" value="{{account.password}}" data-event-opts="{{[['^input',[['__set_model',['$0','password','$event',[]],['account']]]]]}}" bind:__l="__l"></cmd-input></view><view class="register-username"><cmd-input bind:input="__e" vue-id="{{('6')+','+('3')}}" type="text" displayable maxlength="26" placeholder="请输入邮箱" value="{{account.email}}" data-event-opts="{{[['^input',[['__set_model',['$0','email','$event',[]],['account']]]]]}}" bind:__l="__l"></cmd-input></view><button data-event-opts="{{[['tap',[['fnRegister',['$event']]]]]}}" class="btn-register" bindtap="__e">注册</button></view></block></cmd-transition></view></cmd-page-body></view>