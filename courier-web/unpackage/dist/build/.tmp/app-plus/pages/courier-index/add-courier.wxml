<scroll-view class="page" scroll-y><form><view class="cu-form-group margin-top"><view class="title">快递名称</view><input placeholder="快递名称" name="input" data-event-opts="{{[['input',[['__set_model',['$0','courier','$event',[]],['courier']]]]]}}" value="{{courier.courier}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">取件码</view><input placeholder="取件码(我们不会将你的取件码泄露)" name="input" data-event-opts="{{[['input',[['__set_model',['$0','pickupCode','$event',[]],['courier']]]]]}}" value="{{courier.pickupCode}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">送往地址</view><input placeholder="送往地址" name="input" data-event-opts="{{[['input',[['__set_model',['$0','address','$event',[]],['courier']]]]]}}" value="{{courier.address}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">联系电话</view><input placeholder="联系电话" name="input" data-event-opts="{{[['input',[['__set_model',['$0','phone','$event',[]],['courier']]]]]}}" value="{{courier.phone}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">入柜日期</view><picker mode="date" value="{{date}}" start="2015-09-01" end="2020-09-01" data-event-opts="{{[['change',[['DateChange',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+date+''}}</view></picker></view><view class="cu-form-group"><view class="title">选择时间</view><picker mode="time" value="{{time}}" start="09:01" end="21:01" data-event-opts="{{[['change',[['TimeChange',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+time+''}}</view></picker></view><view class="cu-form-group"><view class="title">是否大件</view><switch class="{{[switchA?'checked':'']}}" checked="{{switchA?true:false}}" data-event-opts="{{[['change',[['SwitchA',['$event']]]]]}}" bindchange="__e"></switch></view><view class="cu-form-group align-start"><view class="title">备注</view><textarea maxlength="-1" disabled="{{modalName!=null}}" placeholder="备注" data-event-opts="{{[['input',[['__set_model',['$0','remark','$event',[]],['courier']]]]]}}" value="{{courier.remark}}" bindinput="__e"></textarea></view><button data-event-opts="{{[['tap',[['submit']]]]}}" class="btn-register" bindtap="__e">提交</button></form></scroll-view>